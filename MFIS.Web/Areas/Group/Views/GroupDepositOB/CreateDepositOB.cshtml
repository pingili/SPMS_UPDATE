        function save() {
            var errMsg = ValidateForm();
            if (errMsg != '') {
                alert(errMsg);
                return false;
            }

            var actionUrl = '@Url.Action("SaveDepositOB", "GroupDepositOB")';
            $.post(
                actionUrl,
                $("form").serialize(),
                function (data) {
                    $('#displaysuccess').show();
                    $('#message').html(data.result);
                    $('#Id1').val(data.Id1);
                    $('#Id2').val(data.Id2);
                    $('#Id3').val(data.Id3);
                    $('#Id4').val(data.Id4);
                },
                'json'

                );
        }

        function ValidateForm() {
            var interest1 = $('#Interest1').val();
            var interest2 = $('#Interest2').val();
            if (interest1 == '' && interest2 == '') {
                return "Please select Saving type";
            }

            if (interest1 == interest2) {
                return "Both Saving types should not be same.";
            }

            if (interest1 != '') {
                if ($('#txtDepositBalance1').val() == '') {
                    return "Deposit balace should not be empty.";
                }
            }
            if (interest2 != '') {
                if ($('#txtDepositBalance2').val() == '') {
                    return "Deposit balace should not be empty.";
                }
            }
            return '';
        }

        function getFormattedDateString(dt) {
            var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
            var date = dt;
            var day = date.getDate();
            var monthIndex = date.getMonth();
            var year = date.getFullYear();

            return day + '/' + monthNames[monthIndex] + '/' + year;
        }

