@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_GroupLayout.cshtml";
}

@model MFIS.Web.Areas.Federation.Models.BankMasterModel
<div class="row">
    <div class="col-lg-12">
        @using (Html.BeginForm())
        {
            
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <span class="header">OOPS</span>
                </div>

                <div class="alert alert-danger">

                    <strong>Error!</strong> !OOPS @Session["Message"].ToString()
                </div>
                <div class="alert alert-danger">

                    <strong>Error!</strong> !OOPS Something went wrong, please try again later.
                </div>
                <div class="alert alert-danger">
                    <strong>Error!</strong> !If you face this problem continuously, please contact your administrator
                </div>

                <div class="panel-body">
                </div>
                <div class="panel-footer">
                    <div class="form-group" style="margin-left: 16px;">
                    </div>
                </div>
            </div>
              
        }
    </div>
</div>
<script src="~/Scripts/Common/Common.js"></script>
